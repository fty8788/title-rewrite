<?xml version="1.0" encoding="utf-8" ?>
<titlerewrite>
    <chars> 
        <begin>~!</begin>  
        <end>+-</end>
        <anywhere>#*</anywhere>
    </chars>
    <brackets>  
        <bracket>   
            <text>{},[],(),&lt;&gt;</text>  
            <pos>anywhere</pos>
            <reg>.*</reg>   
        </bracket>
        <bracket>   
            <text>【】,（）,《》</text>  
            <pos>begin,end</pos>
            <file>test_pat1.reg</file>
        </bracket>
    </brackets>
    <element>ch,en,digit,ascii,other</element>    
    <repetition>    
        <len>2</len>
        <times>2</times>
        <action>remove</action>
        <!--<action>reservelast</action>-->
    </repetition>
    <replaces>  
        <replace>   
            <from>|</from>
            <to>。</to>
        </replace>
        <replace>   
            <from>+</from>
            <to>-</to>
        </replace>
    </replaces>
    <outputencoding>gbk</outputencoding>  
</titlerewrite>


